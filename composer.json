{
    "name": "slicksky/domain-blacklist-spam-check",
    "type": "library",
    "description": "Check a domain for blacklisted MX server against most Spam listing services.",
    "keywords": ["blacklist", "spam"],
    "homepage": "https://slicksky.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Kiril Cvetkov",
            "email": "kiril@slicksky.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "SlickSky\\DomainBlacklistSpamCheck\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "larapack/dd": "^1.1",
        "sempro/phpunit-pretty-print": "^1.4"
    }
}
